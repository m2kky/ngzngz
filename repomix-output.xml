                <motion.div
                  layoutId="navBubble"
                  className="absolute w-12 h-12 rounded-full bg-primary nav-bubble-glow"
                  transition={{ type: 'spring', stiffness: 500, damping: 30 }}
                />
              )}
              <Home
                className={cn(
                  'w-6 h-6 relative z-10 transition-colors',
