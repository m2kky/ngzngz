                    </ResponsiveContainer>
                </div>
            </div>

            {/* Assignee Chart */}
            <div className="bg-zinc-950/50 border border-zinc-800 rounded-xl p-6 flex flex-col">
                <h3 className="text-lg font-bold text-white mb-6">Workload Distribution</h3>
