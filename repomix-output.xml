              animate={{
                y: isActive('/intel') ? -20 : 0,
                scale: isActive('/intel') ? 1.1 : 1,
              }}
              whileTap={{ scale: 0.85 }}
              transition={{ type: 'spring', stiffness: 500, damping: 30 }}
