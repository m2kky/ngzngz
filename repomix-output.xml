                                {dosList.map((item, index) => (
                                    <div key={index} className="flex items-center gap-2 text-sm text-zinc-300">
                                        <span className="text-green-400">âœ“</span>
                                        {item}
                                    </div>
                                ))}
                            </div>
                            <div className="flex gap-2">
                                <Input
                                    value={newDo}
                                    onChange={(e) => setNewDo(e.target.value)}
