                  <div className="text-sm font-medium">{attendee.user.full_name || attendee.user.email}</div>
                  <div className="text-[10px] text-muted-foreground">{attendee.role || 'Attendee'}</div>
                </div>
              </div>
              <Badge variant="outline" className="text-[10px] h-5">
